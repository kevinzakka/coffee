<?xml version="1.0" ?>
<robot name="camera">
  <link name="world"/>
    <material name="LightGrey">
      <color rgba="0.7 0.7 0.7 1.0"/>
    </material>
  <link name="base_link">
    <contact>
      <spinning_friction value="0.1"/>
      <lateral_friction value="0.8"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <mass value="0.2"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size=".03 .02 0.01"/>
      </geometry>
      <material name="LightGrey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size=".02 .02 0.01"/>
      </geometry>
      <material name="LightGrey"/>
    </collision>
  </link>

  <!-- Connect camera to world. -->
  <joint name="world_camera_joint" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="1. 0 0.5"/>
  </joint>

</robot>
